{
  "name": "darkrp",
  "type": "module",
  "main": "build/index.js",
  "workspaces": [
    "dashboard"
  ],
  "scripts": {
    "start": "node build/index.js",
    "build:bot": "tsup",
    "build:web": "npm run build --prefix dashboard",
    "build": "concurrently \"npm run build:bot\" \"npm run build:web\"",
    "check": "tsc --noEmit && echo âœ” Ok",
    "bot:dev": "tsx --env-file=.env ./src/index.ts",
    "web:dev": "npm run dev --prefix dashboard",
    "dev": "concurrently --raw --kill-others \"npm run bot:dev\" \"npm run web:dev\"",
    "dev:dev": "tsx --env-file=.env.dev ./src/index.ts",
    "watch": "tsx --watch --env-file=.env ./src/index.ts",
    "watch:dev": "tsx --watch --env-file=.env.dev ./src/index.ts",
    "start:dev": "node --env-file=.env.dev ."
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/static": "^7.0.1",
    "@magicyan/discord": "1.6.3",
    "axios": "^1.13.2",
    "chalk": "5.6.2",
    "concurrently": "^9.2.1",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "fast-glob": "^3.3.3",
    "fastify": "^4.28.1",
    "fastify-socket.io": "^4.0.0",
    "http-status-codes": "2.3.0",
    "mongoose": "^9.0.2",
    "ms": "^2.1.3",
    "rou3": "0.7.10",
    "socket.io": "^4.8.3",
    "tsup": "^8.3.5",
    "tsx": "^4.19.3",
    "typescript": "5.9.3",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "@types/ms": "^2.1.0",
    "@types/node": "^22.16.4"
  },
  "imports": {
    "#env": "./build/env.js",
    "#base": "./build/discord/base/index.js",
    "#functions": "./build/functions/index.js",
    "#database": "./build/database/index.js",
    "#server": "./build/server/index.js",
    "#menus": "./build/menus/index.js",
    "#tools": "./build/tools/index.js",
    "#lib/*": "./build/lib/*.js",
    "#shared/*": "./build/shared/*.js"
  },
  "baseVersion": "1.4.11",
  "overrides": {
    "fastify-socket.io": {
      "fastify": "$fastify"
    }
  }
}